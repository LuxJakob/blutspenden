{
  "title": "Blood Donation Dashboard",
  "panels": [
    {
      "title": "Donation Amount Over Time",
      "type": "timeseries",
      "datasource": "JSON API",
      "targets": [
        {
          "data": "data.json",
          "fields": [
            { "name": "amount_donated_ml", "type": "number" },
            { "name": "donation_date", "type": "time" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ml"
        }
      }
    },
    {
      "title": "Weight vs Donation Amount",
      "type": "scatter",
      "datasource": "JSON API",
      "targets": [
        {
          "data": "data.json",
          "fields": [
            { "name": "amount_donated_ml", "type": "number" },
            { "name": "weight_kg", "type": "number" }
          ]
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ml"
        }
      }
    }
  ]
}
